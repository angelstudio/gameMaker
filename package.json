{
    "name": "game-maker",
    "version": "1.0.0",
    "description": "A game maker powered by Vue.js & Wasm",
    "author": "kllilizxc <541796726@qq.com>",
    "private": true,
    "main": "./dist/electron/main.js",
    "scripts": {
        "dev": "webpack-dev-server --inline --hot --config webpack.config.dev.js",
        "build": "rm -rf dist/* && webpack -p --config webpack.config.prod.js --hide-modules",
        "pack.electron": "cross-env NODE_ENV=production webpack --progress --colors --config webpack.config.main.js",
        "dev.electron": "cross-env NODE_ENV=development webpack --progress --colors --config webpack.config.main.js && npm run dev | electron dist/electron/main.js",
        "build.electron": "npm run build && npm run pack.electron && electron-builder",
        "build.electron.dir": "npm run build && npm run pack.electron && electron-builder --dir",
        "lint": "eslint --ext .js,.vue src"
    },
    "dependencies": {
        "file-dialog": "0.0.6",
        "muse-ui": "^2.1.0",
        "three-js": "^79.0.0",
        "vue": "^2.4.2",
        "vue-router": "^2.7.0",
        "vuex": "^2.4.0"
    },
